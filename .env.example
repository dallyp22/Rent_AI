# PropertyPilotV2 Environment Variables Template
# Copy this file to .env and fill in your actual values

# ============================================
# BACKEND ENVIRONMENT VARIABLES (Railway)
# ============================================

# Database (Railway PostgreSQL - auto-populated by Railway)
DATABASE_URL=postgresql://user:password@host:port/database

# OpenAI API
OPENAI_API_KEY=sk-proj-your-openai-api-key-here

# Firecrawl API (for web scraping)
FIRECRAWL_API_KEY=fc-your-firecrawl-api-key-here

# Session Security (generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))")
SESSION_SECRET=generate-a-random-64-character-hex-string

# Clerk Authentication (get from https://dashboard.clerk.com)
CLERK_SECRET_KEY=sk_test_... or sk_live_...

# Node Environment
NODE_ENV=production

# Server Port
PORT=5000

# Frontend URL (your Vercel deployment URL)
FRONTEND_URL=https://your-app.vercel.app

# ============================================
# FRONTEND ENVIRONMENT VARIABLES (Vercel)
# ============================================

# Backend API URL (your Railway deployment URL)
VITE_API_BASE_URL=https://your-app.up.railway.app

# Clerk Publishable Key (get from https://dashboard.clerk.com)
VITE_CLERK_PUBLISHABLE_KEY=pk_test_... or pk_live_...

# ============================================
# MIGRATION ENVIRONMENT VARIABLES (One-time use)
# ============================================

# Neon Database (source)
NEON_DATABASE_URL=postgresql://neondb_owner:npg_aJLP59YBvDTp@ep-nameless-band-ae5jco0t.c-2.us-east-2.aws.neon.tech/neondb?sslmode=require

# Railway Database (destination)
RAILWAY_DATABASE_URL=postgresql://... (get from Railway dashboard)

